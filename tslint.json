{
    "extends": "tslint:recommended",
    "rulesDirectory": [
        "src/"
    ],
    "rules": {
        "max-line-length": {
            "options": [
                150
            ]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "interface-name": false,
        "quotemark": false,
        "ordered-imports": false,
        "member-access": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "member-ordering": false,
        "max-classes-per-file": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                150
            ]
        },
        "max-classes-per-file": false
    }
}